<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hello.cakeapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowFirebaseData"></activity>
        <activity android:name=".UserGalleryPickUp" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".FinalizeOrder" />
        <activity android:name=".BakerLogIn" />
        <activity
            android:name=".CakeList"
            android:label="Cake List" />
        <activity android:name=".GalleryPickUp" />
        <activity android:name=".CustomisationActivity" />
        <activity android:name=".UserMainActivity" />
        <activity android:name=".BakerActionWin" />
        <activity android:name=".BakerRagister" />
        <activity android:name=".UserRegister" />
        <activity android:name=".UserLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>